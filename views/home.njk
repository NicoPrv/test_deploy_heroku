
{% extends "layout.njk" %}


{% block content %}
<div class="container">
<h1 class="mt-5 text-center pt-4 pb-2">Categories</h1>

<div class="row">
<div class="col-10 offset-1">
<table class="table table-hover table-striped text-center">


  {% for element in myLabels %}


  <tr>


    <td><a style="color:#0082C3;" class="pl-5 pr-5" href="/cat/{{element.id}}">{{element.label}}</a></td>

  </tr>

  {% endfor %}


</table>
</div>
</div>
</div>
<!--<p>{{myLabels}}</p>-->

{% endblock %}
